<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ATMConsultoria" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#9c27b0</item>
        <item name="colorPrimaryVariant">#7b1fa2</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#9c27b0</item>
        <item name="colorSecondaryVariant">#c75fb9</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:colorBackground">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ATMConsultoria.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ATMConsultoria.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ATMConsultoria.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>